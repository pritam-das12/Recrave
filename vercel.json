{
    "version": 2,
    "builds": [
        { "src": "backend/index.js", "use": "@vercel/node" },
        { "src": "backend/dist/**", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "backend/index.js" },
        { "src": "/dist/(.*)", "dest": "backend/dist/$1" }
    ]
}